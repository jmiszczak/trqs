Cell["\<\
Using QuantisRandomRealNormal function we can now define a function for \
generating Ginibre matrix as\
\>", "Text",
 CellChangeTimes->{{3.506525048460073*^9, 3.506525060937225*^9}, {
  3.506525248378839*^9, 3.506525271313977*^9}, {3.506527267502249*^9, 
  3.506527268487865*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"QuantisGinibreMatrix", "[", 
    RowBox[{"m_", ",", "n_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"QuantisRandomRealNormal", "[", 
       RowBox[{"0", ",", "1"}], "]"}], ",", 
      RowBox[{"{", "m", "}"}], ",", 
      RowBox[{"{", "n", "}"}]}], "]"}], "+", 
    RowBox[{"I", " ", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"QuantisRandomRealNormal", "[", 
        RowBox[{"0", ",", "1"}], "]"}], ",", 
       RowBox[{"{", "m", "}"}], ",", 
       RowBox[{"{", "n", "}"}]}], "]"}]}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.506443172150391*^9, 3.506443219662189*^9}, {
  3.506443261004854*^9, 3.506443267874724*^9}}],

Cell["\<\
The function for generating a random density matrix distributed uniformly \
with respect to probability mesure induced by Hilbert-Schmidt metric reads\
\>", "Text",
 CellChangeTimes->{{3.506521324399309*^9, 3.506521374651282*^9}, {
  3.506527284122589*^9, 3.506527291583185*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"QuantisRandomStateHS", "[", "d_", "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", "A", "}"}], ",", "\n", "\t", 
     RowBox[{
      RowBox[{"A", "=", 
       RowBox[{"QuantisGinibreMatrix", "[", 
        RowBox[{"d", ",", "d"}], "]"}]}], ";", "\n", "\t", 
      RowBox[{"A", "=", 
       RowBox[{"(", 
        RowBox[{"A", ".", 
         RowBox[{"ConjugateTranspose", "[", "A", "]"}]}], ")"}]}], ";", "\n", 
      "\t", 
      RowBox[{"A", "=", 
       RowBox[{"Chop", "[", 
        RowBox[{"A", "/", 
         RowBox[{"Tr", "[", "A", "]"}]}], "]"}]}]}]}], "\n", "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.506443145834017*^9, 3.506443150645733*^9}, {
  3.506521398225716*^9, 3.506521399470188*^9}}],

