                  RowBox[{
                   RowBox[{"1", "+", 
                    RowBox[{"n", "*", "ii"}]}], ";;", 
                   RowBox[{
                    RowBox[{"(", 
                    RowBox[{"ii", "+", "1"}], ")"}], " ", "n"}]}]}], "]"}], 
                "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"ii", ",", "0", ",", 
                 RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}], ")"}]}], ",", 
